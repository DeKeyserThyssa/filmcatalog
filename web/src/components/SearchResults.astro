---
    import Card from "./Card.astro";
    
    export let movies: any;
    console.log(movies)
---
    
<ul role="list" class="grid grid-cols-5 gap-x-72 gap-6 m-[70px auto]">
    {
      movies?.map((movie: any) => {
        return (
          <Card
            movie={movie}
          />
        )
      })
    }
  </ul>